<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 <bean class="com.springcore.collections.Employee" name="emp">
 	<property name="empName" value="AnitaKumari"/>
 	<property name="phone">
 		<list>
 			<value>333444</value>
 			<value>342342</value>
 			<value>43434</value>
 			<value>324235</value>
 		</list>
 	</property>
 	
 	<property name="addresses">
 		<set>
 			<value>pune</value>
 			<value>Bihar</value>
 			<value>bihar</value>
 			<value>Saswad</value>
 		</set>
 	</property>
 	
 	<property name="courses">
 		<map>
 			<entry key="java" value="2months"></entry>
  			<entry key="c" value="1month"></entry>
  			<entry key="flutter" value="3months"></entry>
  			<entry key="Android" value="4months"></entry>
 			
